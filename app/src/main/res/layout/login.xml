<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/content_container"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<View
		android:id="@+id/rectangle_2"
		android:layout_width="246dp"
		android:layout_height="46dp"
		android:layout_marginStart="80dp"
		android:layout_marginTop="321dp"
		android:background="@drawable/rectangle_2_shape" />

	<View
		android:id="@+id/rectangle_1"
		android:layout_width="246dp"
		android:layout_height="46dp"
		android:layout_marginStart="80dp"
		android:layout_marginTop="267dp"
		android:background="@drawable/rectangle_1_shape" />

	<TextView
		android:id="@+id/t5ttt"
		android:layout_width="139dp"
		android:layout_height="24dp"
		android:layout_below="@+id/rectangle_2"
		android:layout_marginLeft="80dp"
		android:layout_marginTop="54dp"
		android:fontFamily="Noto Sans"
		android:text="아직 회원이 아니신가요?"
		android:textColor="@color/t5ttt_color"
		android:textSize="12sp" />

	<Button
		android:id="@+id/t4ttt"
		android:layout_width="84dp"
		android:layout_height="30dp"
		android:layout_below="@+id/rectangle_2"
		android:layout_alignStart="@id/t5ttt"
		android:layout_marginStart="165dp"
		android:layout_marginTop="52dp"
		android:background="@color/rectangle_3_color"
		android:fontFamily="Noto Sans"
		android:text="회원가입하기"
		android:textColor="@color/white"
		android:textSize="12sp"
		android:textStyle="bold" />

	<EditText
		android:id="@+id/t2ttt"
		android:layout_width="224dp"
		android:layout_height="wrap_content"
		android:layout_marginLeft="90dp"
		android:layout_marginTop="336dp"
		android:fontFamily="Noto Sans"
		android:hint="비밀번호를 입력하세요"
		android:textColor="@color/t2ttt_color"
		android:textSize="12sp" />

	<EditText
		android:id="@+id/t3ttt"
		android:layout_width="225dp"
		android:layout_height="wrap_content"
		android:layout_marginLeft="90dp"
		android:layout_marginTop="282dp"
		android:fontFamily="Noto Sans"
		android:hint="이메일을 입력하세요"
		android:textColor="@color/t2ttt_color"
		android:textSize="12sp" />

	<ImageView
		android:id="@+id/yocipelogo1"
		android:layout_width="133dp"
		android:layout_height="133dp"
		android:layout_marginStart="140dp"
		android:layout_marginTop="105dp"
		android:contentDescription="yocipelogo1"
		android:src="@drawable/yocipelogo1" />

	<Button
		android:id="@+id/rectangle_3"
		android:layout_width="248dp"
		android:layout_height="30dp"
		android:layout_below="@id/t4ttt"
		android:layout_alignStart="@id/t4ttt"
		android:layout_centerHorizontal="true"
		android:layout_marginStart="-165dp"
		android:layout_marginTop="-71dp"
		android:background="@color/rectangle_3_color"
		android:text="로그인하기"
		android:textColor="@color/white"
		android:textSize="12sp"
		android:textStyle="bold" />

</RelativeLayout>